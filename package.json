{
  "name": "@winner-fed/webpack5-remove-use-strict-plugin",
  "version": "0.0.0",
  "packageManager": "pnpm@10.10.0",
  "description": "Webpack 5 plugin to remove 'use strict' from the generated code",
  "type": "module",
  "license": "MIT",
  "homepage": "https://github.com/winjs-dev/webpack5-remove-use-strict-plugin#readme",
  "bugs": {
    "url": "https://github.com/winjs-dev/webpack5-remove-use-strict-plugin/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/winjs-dev/webpack5-remove-use-strict-plugin.git"
  },
  "author": "winner-fed",
  "files": [
    "dist"
  ],
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      },
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./package.json": "./package.json"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "lint": "eslint --cache .",
    "lint:fix": "pnpm run lint --fix",
    "build": "tsdown",
    "dev": "tsdown --watch",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "format": "prettier --cache --write .",
    "release": "bumpp && pnpm publish",
    "prepublishOnly": "pnpm run build"
  },
  "devDependencies": {
    "@sxzz/eslint-config": "^7.0.1",
    "@sxzz/prettier-config": "^2.2.1",
    "@types/node": "^22.15.17",
    "bumpp": "^10.1.0",
    "eslint": "^9.26.0",
    "prettier": "^3.5.3",
    "tsdown": "^0.11.5",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3",
    "vitest": "^3.1.3",
    "webpack": "^5.99.8"
  },
  "engines": {
    "node": ">=16.18.0"
  },
  "prettier": "@sxzz/prettier-config"
}
